    "def seg(n, df):\n",
    "    df_copy = df.copy()\n",
    "    \n",
    "    # Groupby 聚合方式\n",
    "    groupby_dict = {\n",
    "        \"RegisterSourceTypeDef\": \"first\", \n",
    "        \"OpenCardPresent\": \"first\", \n",
    "        \"IsInBlackList\": \"first\", \n",
    "        \"Female\": \"first\", \n",
    "        \"Male\": \"first\", \n",
    "        \"TrackSourceTypeDef\": \"first\", \n",
    "        \"TrackDeviceTypeDef\": \"first\", \n",
    "        \"PromotionDiscount\": \"sum\", \n",
    "        \"ECouponDiscount\": \"sum\", \n",
    "        \"SalesOrderSlaveTotalPayment\": \"sum\", \n",
    "        \"StatusDef\": \"first\", \n",
    "        \"A\": \"first\", \n",
    "        \"B\": \"first\", \n",
    "        \"C\": \"first\", \n",
    "        \"D\": \"first\", \n",
    "        \"E\": \"first\", \n",
    "        \"F\": \"first\", \n",
    "        \"G\": \"first\", \n",
    "        \"H\": \"first\", \n",
    "        \"I\": \"first\", \n",
    "        \"J\": \"first\", \n",
    "        \"K\": \"first\", \n",
    "        \"L\": \"first\", \n",
    "        \"M\": \"first\", \n",
    "        \"N\": \"first\", \n",
    "        \"O\": \"first\", \n",
    "        \"P\": \"first\", \n",
    "        \"Q\": \"first\", \n",
    "        \"R\": \"first\", \n",
    "        \"S\": \"first\", \n",
    "        \"T\": \"first\", \n",
    "        \"U\": \"first\", \n",
    "        \"V\": \"first\", \n",
    "        \"W\": \"first\", \n",
    "        \"X\": \"first\", \n",
    "        \"Y\": \"first\", \n",
    "        \"Z\": \"first\", \n",
    "        \"AA\": \"first\", \n",
    "        \"AB\": \"first\", \n",
    "        \"num\": \"last\", \n",
    "        \"label\": \"max\"\n",
    "    }\n",
    "    # 建立 Label 欄位\n",
    "    df_copy[\"label\"] = False\n",
    "    df_copy[\"label\"][df_copy[\"num\"]==n+1] = True\n",
    "    \n",
    "    # 將 n+1 以後的數值 (PromotionDiscount&ECouponDiscount&SalesOrderSlaveTotalPayment) 歸零\n",
    "    # 範圍 n+1~num_max\n",
    "    num_max = df_copy[\"num\"].max()\n",
    "    for i in range(n+1, num_max+1):\n",
    "        df_copy[\"PromotionDiscount\"][df_copy[\"num\"]==i] = 0\n",
    "        df_copy[\"ECouponDiscount\"][df_copy[\"num\"]==i] = 0\n",
    "        df_copy[\"SalesOrderSlaveTotalPayment\"][df_copy[\"num\"]==i] = 0\n",
    "    \n",
    "    # 開始 Groupby\n",
    "    data = df_copy.groupby(\"MemberId\").aggregate(groupby_dict)\n",
    "    df_copy = pd.DataFrame(data).reset_index()\n",
    "    \n",
    "    # 去除掉 訂單數(num)<n 的資料\n",
    "    df_copy[df_copy[\"num\"]<n] = np.NAN\n",
    "    df_copy.dropna(inplace=True)\n",
    "    \n",
    "    # 轉換欄位 (PromotionDiscount&ECouponDiscount&SalesOrderSlaveTotalPayment) to (CostAbi&SalesOrderSlaveTotalPayment)\n",
    "    df_copy[\"CostAbi\"] = abs(-(df_copy[\"PromotionDiscount\"] + df_copy[\"ECouponDiscount\"])/(df_copy[\"SalesOrderSlaveTotalPayment\"] - df_copy[\"PromotionDiscount\"] - df_copy[\"ECouponDiscount\"]))\n",
    "    df_copy.drop([\"PromotionDiscount\", \"ECouponDiscount\"], axis=1, inplace=True)\n",
    "    \n",
    "    # 轉換 num欄位 = n\n",
    "    df_copy[\"num\"] = n\n",
    "    df_copy.to_csv(\"./data/segData/df_seg{0}.csv\".format(n))\n",
    "    \n",
    "    return df_copy"
   ]
  },
